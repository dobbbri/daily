  "tokenColors": [
    {
      "settings": {
        "foreground": "#f8f8f2ff",
        "background": "#0b1015"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#7E8E91"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded.coffee"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
